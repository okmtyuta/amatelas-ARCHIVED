{
  "name": "@okmtyuta/_amatelas-theme",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:theme": "vite build && tsc && tsc-alias",
    "build:css": "pnpm ts-node ./src/generator/generate.ts",
    "build": "pnpm build:theme && pnpm build:css",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./color": {
      "import": "./dist/color.js",
      "types": "./dist/types/theme/color/color.d.ts"
    },
    "./prefixed": {
      "import": "./dist/prefixed.js",
      "types": "./dist/types/prefixed/index.d.ts"
    }
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
